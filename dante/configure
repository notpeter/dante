      perror("socket");
      exit(1);
   }

   optval = 1;
   optlen = sizeof(optval);
   if(setsockopt(s, IPPROTO_IPV6, IPV6_RECVTCLASS, &optval, optlen) < 0) {
      perror("setsockopt: IPPROTO_IPV6 IPV6_RECVTCLASS");
      close(s);
      exit(1);
   }
  ;
  return 0;
