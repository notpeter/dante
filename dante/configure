$as_echo "no" >&6; }
       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: assuming buffercache not unified on this platform" >&5
$as_echo "$as_me: WARNING: assuming buffercache not unified on this platform" >&2;}
